<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="./index.css">
    <title>Ingreso de Productos</title>
</head>
<body>
    <div class="productContainer">
        <h2 class="title">Vista de productos</h2>
        <% if(productos.length) { %>


        
                    <div class="titleHeader"><h5 class="titleNombre">Nombre</h5><h5 class="titlePrecio">Precio</h5><h5 class="titleFoto">Foto URL</h5></div>
                    
                    <% productos.forEach((product) => { %>
                        <div class="productData">
                            <div class="productTitle"><h5><%= product.title %></h5></div>
                            <div class="productPrice"><h5><%= product.price %></h5></div>
                            <div class="productThumbnail"><img src=<%= product.thumbnail %> alt=""></img></div>
                        </div>
                    <%
                    } ) %>  
                  
                        
            


        <% } %>

        <a href="/"><button class="css-button-sliding-to-top--blue2">Volver al formulario de carga</button></a>
    </div>
</body>
</html>

















